{
  "to": "https://webhook.site/your-webhook-id",
  "method": "POST",
  "headers": {
    "Authorization": "Bearer {{env.TOKEN}}",
    "X-Custom-Header": "{{query.custom}}",
    "Content-Type": "application/json; charset=UTF-8",
    "User-Agent": "ewhooks/1.0.0"
  },
  "body": {
    "forwarded_message": "{{body.message}}",
    "user_name": "{{body.user.name}}",
    "from_header": "{{headers.X-Original-Header}}",
    "timestamp": "{{body.timestamp}}"
  }
}