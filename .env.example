# 端口配置
PORT=3000

# 日志配置
# 日志级别: INFO, WARN, ERROR (默认: INFO)
LOG_LEVEL=INFO

# 允许打印请求载体的 adapter 列表,逗号分隔 (留空则全部打印)
# 例如: BODY_LOG_ADAPTERS=example,webhook1,webhook2
BODY_LOG_ADAPTERS=

# 允许打印转发载体的 adapter 列表,逗号分隔 (留空则全部打印)
# 例如: FORWARD_LOG_ADAPTERS=example,webhook1,webhook2
FORWARD_LOG_ADAPTERS=

# 是否打印请求头 (默认: false)
LOG_HEADERS=false

# 示例环境变量(用于配置文件中的 {{env.VARIABLE_NAME}})
# TOKEN=your_secret_token_here
# API_KEY=your_api_key_here
